
public class temp {
	public static void main(String[] args) {
		int n = 100;
		int[][] g = new int[n][n];
		System.out.println(1);
		System.out.println(n);
		for (int i = 0; i < n; i++) {
			for (int j = 0; j < n; j++) {
				if (j == i+1) {
					g[i][j] = 1;
				} else {
					g[i][j] = 0;
				}
			}
		}
	}
}
